        </style>
        <script>
            const items = document.querySelectorAll(".drop")
            Array.from(items).forEach((element) => {
                let sumMenuEl = element.querySelector(".drop__submenu")
                let dropTitle = element.querySelector(".drop__title")
                let svg = element.querySelector("svg")
                const handleclick = () => {
                    sumMenuEl.classList.toggle("active")
                    svg.classList.toggle("active")
                }
                dropTitle.addEventListener("click", handleclick)

            })
            let burgerElement = document.querySelector('.header_burger')
            // let closeElement = document.querySelector('.close')
            burgerElement.onclick = burgerMenu;
            // closeElement.onclick = burgerMenu;
            function burgerMenu(event) {
                let menuElement = document.querySelector('menu')
                let bodylement = document.querySelector('body')
                menuElement.classList.toggle("active");
                bodylement.classList.toggle("lock");
                // closeElement.classList.toggle("active")
                burgerElement.classList.toggle("active")

            }
            const seach_input = document.querySelector("#seach_input")
            const moduleEl = document.querySelectorAll(".module__title")
            const funcTitle = document.querySelectorAll(".func__title")
            const funcDecription = document.querySelectorAll(".func__description")
            const template_menu = document.querySelector("#template-search")
            const searchMenu = document.querySelector(".search-menu")
            const listSeach = []
            const exitButton = document.querySelector(".searh__exit")

            exitButton.addEventListener("click", (e) => {
                searchMenu.innerHTML = ""
                seach_input.value = ""
                searchMenu.style.display = "none"
            })
            Array.from(moduleEl).forEach((element) => listSeach.push({
                type: "module",
                text: element.innerText,
                href: "#" + element.dataset.href
            }))
            Array.from(funcTitle).forEach((element) => listSeach.push({
                type: "function",
                text: element.innerText,
                module: element.dataset.module,
                href: "#" + element.dataset.href
            }))

            Array.from(funcDecription).forEach((element) => {
                if (element.module)
                    listSeach.push({
                        type: "function",
                        text: element.innerText,
                        module: element.dataset.module,
                        href: "#" + element.dataset.href
                    })
            })
            searchMenu.style.display = "none"
            console.log(listSeach)
            seach_input.addEventListener("input", (e) => {
                searchMenu.style.display = "flex"
                searchMenu.innerHTML = ""
                const textQuery = e.target.value.toLowerCase()

                if (textQuery) {
                    listSeach.filter((el) => el.text.toLowerCase().includes(textQuery)).forEach((el) => {
                        const clone = template_menu.content.cloneNode(true);
                        console.log(clone.href)
                        const title = clone.querySelector(".title")
                        const module = clone.querySelector(".module")
                        const ling = clone.querySelector("a").href = el.href

                        if (el.type == "function") {
                            title.innerText = el.text
                            module.innerText = el.module
                            searchMenu.append(clone)
                        }
                        if (el.type == "module") {
                            title.innerText = el.text
                            searchMenu.append(clone)
                        }
                    })

                }
                else {
                    searchMenu.style.display = "none"
                }
            })
        </script>
